<div class="container">
    <div class="mt-4"> 
    <div class="row">
        {{#each produces}}
        <div class="col-sm-6 col-lg-4">
                <div class="card" style="width: 18rem;">
                  <a href="/muahang/{{this.slug}}">
                <img class="card-img-top" src="{{this.image}}" alt="{{this.name}}">
                  </a>
                <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">{{this.decreption}}</p>
                <a href="/muahang/{{this.slug}}" class="btn btn-primary">Mua ngay</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>
</div>


